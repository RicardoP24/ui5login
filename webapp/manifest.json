{
    "_version": "1.58.0",
    "sap.app": {
        "id": "ui5.walkthrough",
        "_version": "1.1.0",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": [
                "{{<keyword1>}}",
                "{{<keyword2>}}"
            ]
        }
    },
    "sap.ui5": {
        "resources": {
            "css": [
                {
                    "uri": "css/styles.css"
                }
            ]
        },
        "rootView": {
            "viewName": "ui5.walkthrough.view.App",
            "type": "XML"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "ui5.walkthrough.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "login",
                    "target": "login"
                },
                {
                    "pattern": "home",
                    "name": "home",
                    "target": "home"
                }
            ],
            "targets": {
                "login": {
                    "viewName": "Login",
                    "viewLevel": 1
                },
                "home": {
                    "viewName": "Home",
                    "viewLevel": 2
                }
            }
        }
    }
}